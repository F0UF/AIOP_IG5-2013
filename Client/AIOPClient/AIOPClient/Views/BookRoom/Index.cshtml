@{
    ViewBag.Title = "Book Room";
}



<form role="form">
  <div class="form-group">
    <label for="promotion">Promotion</label>
    <select class="form-control">
    </select>
  </div>
  <div class="form-group">
    <label for="year">Year</label>
    <select class="form-control">
    </select>
  </div>
  <div class="form-group">
    <label for="group">Group</label>
    <select class="form-control">
    </select>
  </div>
  <div class="form-group">
    <label for="teaching">Teaching</label>
    <select class="form-control">
    </select>
  </div>
  <div class="form-group">
    <label for="type">Type</label>
    <select class="form-control">
    </select>
  </div>
  <div class="checkbox">
    Characteristics: 
    <label class="checkbox-inline">
        <input type="checkbox" id="projector" value="option1"> Projector
    </label>
    <label class="checkbox-inline">
        <input type="checkbox" id="computers" value="option2"> Computers
    </label>
  </div>
    <div id="datePicker" class="input-append date">
      <input type="text"></input>
      <span class="add-on">
        <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
      </span>
    </div>
    <script type="text/javascript"
     src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js">
    </script> 
    <script type="text/javascript"
     src="http://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.pt-BR.js">
    </script>
    <script type="text/javascript">
        $('#datePicker').datetimepicker({
            format: 'dd/MM/yyyy',
            language: 'en',
            pickTime: false
        });
    </script>
    <div id="beginTimePicker" class="input-append date">
      <input type="text"></input>
      <span class="add-on">
        <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
      </span>
    </div>
    <script type="text/javascript"
     src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js">
    </script> 
    <script type="text/javascript"
     src="http://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.pt-BR.js">
    </script>
    <script type="text/javascript">
        $('#beginTimePicker').datetimepicker({
            format: 'hh:mm:ss',
            language: 'en',
            pickDate: false,
            pickSeconds: false
        });
    </script>
    <div id="endTimePicker" class="input-append date">
      <input type="text"></input>
      <span class="add-on">
        <i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
      </span>
    </div>
    <script type="text/javascript"
     src="http://cdnjs.cloudflare.com/ajax/libs/jquery/1.8.3/jquery.min.js">
    </script> 
    <script type="text/javascript"
     src="http://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.min.js">
    </script>
    <script type="text/javascript"
     src="http://tarruda.github.com/bootstrap-datetimepicker/assets/js/bootstrap-datetimepicker.pt-BR.js">
    </script>
    <script type="text/javascript">
        $('#endTimePicker').datetimepicker({
            format: 'hh:mm:ss',
            language: 'en',
            pickDate: false,
            pickSeconds: false
        });
    </script>
  <button type="submit" class="btn btn-default">Submit</button>
</form>